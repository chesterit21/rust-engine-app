# Database
DATABASE_URL=postgres://user:password@localhost:5432/dbname
DB_POOL_MAX_SIZE=10

# Embedding
EMBEDDING_MODEL=AllMiniLML6V2
EMBEDDING_DIMENSION=384
CHUNK_SIZE=512
CHUNK_OVERLAP=50

# Worker
WORKER_THREADS=4
BATCH_SIZE=10
MAX_RETRIES=3

# Logging
RUST_LOG=info,rag_embedding_worker=debug
LOG_FORMAT=json

# Paths
DOCUMENT_ROOT_PATH=/path/to/documents
